<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Guest.
    -->
    <changeSet id="20230227074637-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="payment_grade_id"
                                 baseTableName="guest"
                                 constraintName="fk_guest__payment_grade_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_grade"/>

        <addForeignKeyConstraint baseColumnNames="installation_id"
                                 baseTableName="guest"
                                 constraintName="fk_guest__installation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="installation"/>

        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="guest"
                                 constraintName="fk_guest__department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>
    </changeSet>
</databaseChangeLog>
